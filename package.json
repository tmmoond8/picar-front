  },
  "scripts": {
    "zip:prerender": "cd build && zip -r ../public/prerender static article notion notice qna search",
    "unzip:prerender": "unzip -n public/prerender.zip -d build/",
    "dev": "PORT=8200 react-app-rewired start",
    "dev:ios": "ionic capacitor run ios -l --external",
    "dev:android": "ionic capacitor run android -l --external",
    "dev:lambda": "netlify-lambda serve src/functions",
    "babel:serverless": "yarn babel --config-file ./babel.serverless.json --out-dir lambda ./src --extensions \".ts,.tsx,.svg\"",
    "build": "CI= react-app-rewired build && mv build/index.html build/server.html",
    "build:netlify": "REACT_APP_GIT_HASH=$COMMIT_REF yarn build && yarn build:lambda",
    "build:mobile": "dotenv -e .env.production.env react-app-rewired build && capacitor copy ios && capacitor copy android",
    "build:local": "react-app-rewired build && capacitor copy ios && capacitor copy android",
    "build:serverless": "yarn babel:serverless && yarn tool:replace-svg-to-js && yarn tool:remove-env-d-js",
    "pre:prerender": "rm .env && cp .env.production.env .env",
    "prerender": "yarn pre:prerender && yarn build && react-snap && yarn zip:prerender yarn post:prerender",
    "post:prerender": "rm .env && mv .env_b .env",
    "cp:svg": "cp src/components/Icon/icons/*.svg lambda/components/Icon/icons/",
    "tool:resources": "cordova-res ios && cordova-res android && node resources/script.js",
    "tool:replace-svg-to-js": "yarn replace-in-files --string='.svg' --replacement='.js' lambda/components/Icon/icons/index.js",
    "tool:remove-env-d-js": "rm lambda/react-app-env.d.js",
    "eject": "react-app-rewired eject"
  },
  "reactSnap": {
    "include": [
      "/prerendering"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
